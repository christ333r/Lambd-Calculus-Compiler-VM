{
  "name": "lts-support",
  "displayName": "Lts-Support",
  "description": "color y gramatica para lts",
  "publisher": "christ-dev",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "LTS",
      "aliases": ["Lts", "LTS"],
      "extensions": [".lts"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "LTS",
      "scopeName": "source.lts",
      "path": "./syntaxes/LTS.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "LTS",
        "path": "./snippets/lts.json"
      }
    ]
  }
}
